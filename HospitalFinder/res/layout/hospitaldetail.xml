<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/mainLayout"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <TextView
        android:id="@+id/topBackground"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:background="#ccf8ff" >
    </TextView>

    <ImageView
        android:id="@+id/goBackIcon"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:background="#ccf8ff"
        android:paddingLeft="10dp"
        android:src="@android:drawable/ic_menu_revert" />

    <ImageView
        android:id="@+id/goMapIcon"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:paddingRight="10dp"
        android:src="@android:drawable/ic_menu_mylocation" />

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@+id/TextView1"
        android:fillViewport="true"
        android:paddingBottom="50dp" >

        <LinearLayout
            android:id="@+id/subLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/ll1"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <ImageView
                    android:id="@+id/hospitalImage"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_marginTop="30dp"
                    android:adjustViewBounds="true"
                    android:alpha=".35"
                    android:src="@drawable/hmc" />

                <TextView
                    android:id="@+id/hospitalName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Harborview Medical Center"
                    android:textSize="23dp" />

                <RatingBar
                    android:id="@+id/ratingBar"
                    style="?android:attr/ratingBarStyleSmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/textHospital"
                    android:layout_margin="5dp"
                    android:numStars="4"
                    android:paddingTop="5dp"
                    android:rating="4"
                    android:stepSize="1" />

                <TextView
                    android:id="@+id/reviewNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textHospital"
                    android:layout_toRightOf="@+id/ratingBar1"
                    android:text="21 Reviews"
                    android:textColor="#9e9e9e"
                    android:textSize="18dp" />

                <TextView
                    android:id="@+id/distance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textHospital"
                    android:layout_toRightOf="@+id/textReview"
                    android:paddingLeft="15dp"
                    android:text="2.7 mi"
                    android:textSize="20dp" />

                <ImageView
                    android:id="@+id/callImage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/ratingBar1"
                    android:layout_below="@+id/ratingBar1"
                    android:layout_centerVertical="true"
                    android:src="@android:drawable/ic_menu_call" />

                <TextView
                    android:id="@+id/phoneNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ratingBar1"
                    android:layout_toRightOf="@+id/imageView8"
                    android:paddingLeft="10dp"
                    android:text="(206) 520-5000"
                    android:textSize="19dp" />

                <TextView
                    android:id="@+id/price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/phoneNumber"
                    android:layout_below="@+id/ratingBar1"
                    android:layout_toRightOf="@+id/phoneNumber"
                    android:paddingLeft="30dp"
                    android:text="$60"
                    android:textColor="#04bd53"
                    android:textSize="25dp" />

                <TextView
                    android:id="@+id/duration"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/ratingBar1"
                    android:layout_toRightOf="@+id/price"
                    android:paddingLeft="5dp"
                    android:text="/ 30 min"
                    android:textSize="19dp" />

                <ImageView
                    android:id="@+id/directionImage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/ratingBar1"
                    android:layout_below="@+id/phoneNumber"
                    android:layout_centerVertical="true"
                    android:src="@android:drawable/ic_menu_directions" />

                <TextView
                    android:id="@+id/address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/phoneNumber"
                    android:layout_toRightOf="@+id/imageView9"
                    android:paddingLeft="10dp"
                    android:text="325 9th Ave, Seattle, WA 98104"
                    android:textSize="19dp" />

                <ImageView
                    android:id="@+id/openhoursImage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/ratingBar1"
                    android:layout_below="@+id/address"
                    android:src="@android:drawable/ic_menu_my_calendar" />

                <TextView
                    android:id="@+id/openhours"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/address"
                    android:layout_toRightOf="@+id/imageView10"
                    android:paddingLeft="10dp"
                    android:text="Mon-Fri\t8:00 am - 4:30 pm\nSat-Sun\tClosed"
                    android:textSize="19dp" />

                <ImageView
                    android:id="@+id/websiteImage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/ratingBar1"
                    android:layout_below="@+id/openDays"
                    android:src="@android:drawable/ic_menu_info_details" />

                <TextView
                    android:id="@+id/website"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/openDays"
                    android:layout_toRightOf="@+id/imageView12"
                    android:autoLink="web"
                    android:clickable="true"
                    android:paddingLeft="10dp"
                    android:text="uwmedicine.org/harborview"
                    android:textSize="19dp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/review"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Review"
                    android:textColor="#000000"
                    android:textSize="18dp" />

                <TextView
                    android:id="@+id/bookmark"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Bookmark"
                    android:textColor="#000000"
                    android:textSize="18dp" />

                <TextView
                    android:id="@+id/appointment"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Appointment"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginTop="10dp"
                    android:background="#c7d3ab" />

                <TextView
                    android:id="@+id/features"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="10dp"
                    android:paddingLeft="15dp"
                    android:paddingTop="10dp"
                    android:text="Features"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll41"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/featureImage1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/text3"
                    android:paddingLeft="15dp"
                    android:src="@android:drawable/btn_star_big_on" />

                <TextView
                    android:id="@+id/feature1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingRight="10dp"
                    android:text="Operation Smile"
                    android:textSize="16dp" />
                
                <LinearLayout
                    android:id="@+id/ll51"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/featureContent1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Free surgery for all children born with a cleft lip or cleft palate" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <View
                    android:id="@+id/text4"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginTop="10dp"
                    android:background="#c7d3ab" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="10dp"
                    android:paddingLeft="15dp"
                    android:paddingTop="10dp"
                    android:text="Specialty"
                    android:textColor="#000000"
                    android:textSize="18dp" />

                <LinearLayout
                    android:id="@+id/ll7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/specialtyImage1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="15dp"
                        android:src="@android:drawable/ic_menu_view" />

                    <TextView
                        android:id="@+id/specialty1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Eye Care"
                        android:textSize="16dp" />

                    <ImageView
                        android:id="@+id/specialtyImage2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingLeft="25dp"
                        android:src="@android:drawable/ic_menu_view" />

                    <TextView
                        android:id="@+id/specialty2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Neurosciences"
                        android:textSize="16dp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <View
                    android:id="@+id/text7"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginTop="10dp"
                    android:background="#c7d3ab" />

                <TextView
                    android:id="@+id/review"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="10dp"
                    android:paddingLeft="15dp"
                    android:paddingTop="10dp"
                    android:text="Review"
                    android:textColor="#000000"
                    android:textSize="18dp" />

                <LinearLayout
                    android:id="@+id/ll9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/reviewIcon1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_sad" />

                    <TextView
                        android:id="@+id/reviewContent1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Told me to go to the VA for help with a small issue. I just needed some antibiotics and I was paying cash? Thanks a lot for making me wait 4 hours just to tell me that I must go to the VA.\n" >
                    </TextView>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/reviewIcon2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_smile" />

                    <TextView
                        android:id="@+id/reviewContent2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Love these docs who care to make me better. My pain doctor who is an anesthesiologist figured out what my issue was and is aggresively fixing it." >
                    </TextView>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/ll11"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:background="#ccf8ff"
        android:gravity="center"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/goMap"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@android:drawable/ic_menu_mylocation" />

        <ImageView
            android:id="@+id/goFilter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@android:drawable/ic_menu_zoom" />

        <ImageView
            android:id="@+id/goList"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@android:drawable/ic_menu_sort_by_size" />

        <ImageView
            android:id="@+id/goWhat"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:src="@android:drawable/ic_menu_recent_history" />
    </LinearLayout>

</RelativeLayout>